{
  "strategy_name": "voting_ensemble",
  "version": "v13",
  "description": "VWAP + BREAKOUT + Stochastic 투표 방식 앙상블",
  "timeframe": "day",

  "algorithms": {
    "vwap": {
      "weight": 2.0,
      "params": {
        "deviation_threshold": 0.02
      }
    },
    "breakout": {
      "weight": 1.5,
      "params": {
        "period": 20
      }
    },
    "stochastic": {
      "weight": 1.0,
      "params": {
        "k_period": 14,
        "d_period": 3,
        "oversold": 20,
        "overbought": 80
      }
    }
  },

  "vote_threshold": 3.0,
  "trailing_stop": 0.20,
  "stop_loss": 0.10,
  "initial_capital": 10000000,
  "fee_rate": 0.0005,
  "slippage": 0.0002
}
