{
  "strategy_name": "supreme",
  "version": "v34",
  "timeframe": "day",
  "description": "2020-2024 데이터 기반 Multi-Strategy 통합",

  "market_classifier": {
    "mfi_bull_strong": 52,
    "mfi_bull_moderate": 45,
    "mfi_sideways_up": 42,
    "mfi_sideways_flat": 38,
    "mfi_sideways_down": 35,
    "mfi_bear_moderate": 38,
    "mfi_bear_strong": 35,
    "adx_strong_trend": 20,
    "adx_moderate_trend": 15,
    "adx_weak_trend": 12
  },

  "momentum_trading": {
    "rsi_bull_strong": 52,
    "rsi_bull_moderate": 55
  },

  "breakout_trading": {
    "lookback_period": 20,
    "breakout_threshold": 0.005,
    "volume_multiplier": 1.3
  },

  "range_trading": {
    "lookback_period": 20,
    "support_zone": 0.15,
    "rsi_oversold": 40
  },

  "risk_management": {
    "take_profit_1": 0.02,
    "take_profit_2": 0.05,
    "take_profit_3": 0.10,
    "stop_loss": -0.015
  },

  "position_sizing": {
    "position_size": 0.5,
    "kelly_enabled": false,
    "kelly_multiplier": 0.25
  },

  "backtesting": {
    "initial_capital": 10000000,
    "fee_rate": 0.0005,
    "slippage": 0.0002,
    "train_period": "2020-01-01 to 2024-12-31",
    "test_period": "2025-01-01 to 2025-10-17"
  },

  "target_performance": {
    "annual_return": 0.20,
    "sharpe_ratio": 1.5,
    "max_drawdown": 0.20,
    "win_rate": 0.50
  }
}
